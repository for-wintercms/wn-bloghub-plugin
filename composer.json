    "keywords": [
        "winter",
        "wintercms",
        "blog",
        "meta",
        "archives"
    ],
    "homepage": "https://github.com/for-wintercms/wn-bloghub-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam",
            "email": "sam@rat.md",
            "role": "Developer"
        },{
            "name": "DiamondSystems",
            "email": "me@diamondsystems.org",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "winter-cms@diamondsystems.org",
        "issues": "https://github.com/for-wintercms/wn-bloghub-plugin/issues",
        "source": "https://github.com/for-wintercms/wn-bloghub-plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 7.4 || >= 8.0",
        "composer/installers": "~1.0",
        "gregwar/captcha": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
